    "def conv(n):\n",
    "    print(n)\n",
    "    while n > 1:\n",
    "        if n % 2 == 0 :\n",
    "            n = n//2\n",
    "            print (n)\n",
    "        else:\n",
    "            n = 3*n+1\n",
    "            print (n)\n",
    "        \n",
    "n = 100\n",
    "conv(n)"
   ]
